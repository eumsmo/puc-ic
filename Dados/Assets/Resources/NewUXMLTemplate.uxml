                <ui:Label tabindex="-1" text="1/10" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Rodadas" style="-unity-font-style: bold; -unity-text-align: middle-right; flex-grow: initial; font-size: 30px; color: rgb(243, 243, 243);" />
