            <ui:Button text="Jogar" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ComecarButton" class="botao" style="padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px; font-size: 20px; width: 22%; flex-grow: 0; background-color: rgb(142, 130, 50);" />
