            </ui:VisualElement>
            <ui:VisualElement name="Middle" style="flex-grow: 1;">
                <ui:VisualElement name="Holder" style="flex-grow: 1; background-color: rgb(204, 203, 146); width: 100%; height: 100%; flex-direction: column; flex-wrap: wrap; align-items: center; justify-content: center; align-self: center; padding-top: 8px; padding-right: 8px; padding-bottom: 8px; padding-left: 8px;">
                    <ui:VisualElement name="Graficos" style="flex-grow: 1; flex-shrink: 0; width: 100%; justify-content: center; align-items: center; display: flex;">
                        <ui:Label tabindex="-1" text="Quantos porcento de alguma coisa" parse-escape-sequences="true" display-tooltip-when-elided="true" name="GraficosTexto" style="white-space: normal; -unity-text-align: upper-center; color: rgb(0, 0, 0); width: 80%; margin-bottom: 12px;" />
                        <ui:VisualElement name="Grafico" style="flex-grow: 0; flex-direction: row; height: 144px; width: 109px; justify-content: space-between;">
                            <ui:Slider label="Slider" high-value="100" direction="Vertical" name="SliderGrafico" class="escolher_porcentagem vertical limpo" style="width: 14px; height: 100%;" />
                            <ui:Slider label="Slider" high-value="100" direction="Vertical" name="SliderGrafico2" class="escolher_porcentagem vertical limpo" style="width: 14px; height: 100%;" />
                            <ui:Slider label="Slider" high-value="100" direction="Vertical" name="SliderGrafico3" class="escolher_porcentagem vertical limpo" style="width: 14px; height: 100%;" />
