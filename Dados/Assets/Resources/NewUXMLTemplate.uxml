        <ui:VisualElement name="StartScreen" class="conteudo-holder" style="flex-grow: initial; position: absolute; top: 0; background-color: rgb(184, 182, 116); display: none; align-items: center; justify-content: center; padding-top: 40px; padding-right: 40px; padding-bottom: 40px; padding-left: 40px; align-self: auto;">
            <ui:VisualElement style="flex-grow: 1; justify-content: center; align-items: center;">
                <ui:Label tabindex="-1" text="Descubra o Título" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Label" style="font-size: 34px; -unity-font-style: bold; display: none;" />
                <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Label" style="width: 300px; white-space: normal; -unity-text-align: upper-center; font-size: 18px;" />
