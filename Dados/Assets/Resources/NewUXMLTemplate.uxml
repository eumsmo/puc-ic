                    </ui:VisualElement>
                    <ui:VisualElement name="Porcentagem" style="flex-grow: 1; flex-shrink: 0; width: 100%; justify-content: center; align-items: center; display: flex;">
                        <ui:Label tabindex="-1" text="Quantos porcento de alguma coisa" parse-escape-sequences="true" display-tooltip-when-elided="true" name="PorcentagemTexto" style="white-space: normal; -unity-text-align: upper-center; color: rgb(0, 0, 0); width: 80%; margin-bottom: 12px;" />
                        <ui:Slider label="Slider" high-value="100" direction="Horizontal" name="SliderPorcentagem" class="escolher_porcentagem horizontal" style="width: 70%;" />
                        <ui:Button text="Confirmar" parse-escape-sequences="true" display-tooltip-when-elided="true" name="PorcentagemConfirmar" class="botao" style="width: auto; height: 29px; margin-top: 16px;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="Boleano" style="flex-grow: 1; flex-shrink: 0; width: 100%; justify-content: center; align-items: center; max-width: 80%; display: flex;">
                        <ui:Label tabindex="-1" text="É possível fazer isso e aquilo todo dia até não dar mais ou nem a pau?" parse-escape-sequences="true" display-tooltip-when-elided="true" name="BoleanoTexto" style="white-space: normal; -unity-text-align: upper-center; color: rgb(0, 0, 0);" />
                        <ui:VisualElement style="flex-grow: 0; flex-direction: row; margin-top: 8px;">
                            <ui:Button text="Sim" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ButtonSim" class="botao" style="width: auto; height: 29px;" />
                            <ui:Button text="Não" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ButtonNao" class="botao" style="width: auto; height: 29px; background-color: rgb(190, 59, 58);" />
